package edu.psu.ist.ab.ist242.team1;

enum PartID{name}

public class Parts {

    private int partID;
    private String  name;
    private String partCategory;

    public Parts(int _partID, String _partCategory, String _name){
        super();
        this.name = _name;
        this.partCategory = _partCategory;
        this.partID = _partID;
    }

    public Parts() { ++partID;}


    public int getPartID(){
        return partID;
    }
    public void setPartID(int randomID){this.partID = randomID;}

    public String getName(){return name;}

    public void setName(String partName){this.name = partName;}

    public String getPartCategory() {
        return partCategory;
    }
    public void setPartCategory(String _partCategory){this.partCategory = _partCategory;}
}

